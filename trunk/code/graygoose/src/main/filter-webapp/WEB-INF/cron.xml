<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <!--Normal mode jobs-->
    <cron>
        <url>/cron/sites?action=checkSites</url>
        <description>Check sites</description>
        <schedule>every 1 minutes</schedule>
    </cron>
    <cron>
        <url>/cron/events?action=removeOld</url>
        <description>Remove old events with 'SUCCESS' status from the data storage</description>
        <schedule>every 30 minutes</schedule>
    </cron>

