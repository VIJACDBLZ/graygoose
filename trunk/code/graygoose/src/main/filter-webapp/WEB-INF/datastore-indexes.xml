<?xml version="1.0" encoding="UTF8"?>
<datastore-indexes autoGenerate="true">

    <!--Alert-->
    <datastore-index kind="Alert" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="name" direction="asc"/>
    </datastore-index>

    <!--Rule-->
    <datastore-index kind="Rule" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="siteId" direction="asc"/>
        <property name="creationTime" direction="desc"/>
    </datastore-index>

    <!--Site-->
    <datastore-index kind="Site" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="creationTime" direction="desc"/>
    </datastore-index>

    <!--AlertTriggerEvent-->
    <datastore-index kind="AlertTriggerEvent" ancestor="false" source="manual">
        <property name="alertId" direction="asc"/>
        <property name="deleted" direction="asc"/>
        <property name="creationTime" direction="asc"/>
    </datastore-index>

    <!--RuleCheckEvent-->
    <datastore-index kind="RuleCheckEvent" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="ruleId" direction="asc"/>
        <property name="checkTime" direction="asc"/>
    </datastore-index>

    <datastore-index kind="RuleCheckEvent" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="checkTime" direction="asc"/>
    </datastore-index>

    <datastore-index kind="RuleCheckEvent" ancestor="false" source="manual">
        <property name="deleted" direction="asc"/>
        <property name="status" direction="asc"/>
        <property name="checkTime" direction="asc"/>
    </datastore-index>

</datastore-indexes>