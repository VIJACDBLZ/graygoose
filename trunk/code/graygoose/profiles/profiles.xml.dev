<?xml version="1.0" encoding="UTF8"?>
<profilesXml>
    <profiles>
        <profile>
            <id>development-settings</id>
            <properties>
                <!-- Directory to the root of the project. -->
                <development.home>P:\graygoose\code/graygoose</development.home>
                
                <application.name>cc-graygoose</application.name>

                <!-- True iff you want to use the debug mode. -->
                <nocturne.debug>true</nocturne.debug>

                <!-- Path to write log file. -->
                <logging.path>${development.home}/graygoose.log</logging.path>

                <!-- Sender email address. -->
                <mail.from>test@gmail.com</mail.from>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>development-settings</activeProfile>
    </activeProfiles>
</profilesXml>
