<?xml version="1.0" encoding="UTF8"?>
<profilesXml>
    <profiles>
        <profile>
            <id>production-settings</id>
            <properties>
                <!-- Not used in the production mode. -->
                <development.home/>

                <application.name>public-graygoose</application.name>

                <!-- Do not use debug mode. -->
                <nocturne.debug>false</nocturne.debug>

                <!-- Path to write log file. -->
                <logging.path>graygoose.log</logging.path>
                
                <!-- Sender email address. -->
                <mail.from>test@gmail.com</mail.from>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>production-settings</activeProfile>
    </activeProfiles>
</profilesXml>
